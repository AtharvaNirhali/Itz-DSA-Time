#include<bits/stdc++.h>
using namespace std;

class Qimpl{
  int size = 10;
  int q[size];
  int curr_size = 0;
  int start = -1;
  int end = -1;

  void push(int x){
    if(curr_size == size)
      cout<<"Queue is full!";
    if(curr_size == 0){
      start = 0;
      end = 0;
    }
    else
      end = (end+1)%size;
    q[end] = x;
    curr_size += 1;
  }

  int top(){
    if(curr_size == 0)
      return 0;
    return q[start];
  }

  int size(){
    return curr_size;
  }

};
