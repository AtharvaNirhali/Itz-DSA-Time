#include<bits/stdc++.h>
using namespace std;

class st_impl{
  int top = -1;
  int st[10];

  void push(int x){
    if(top>=10)
      cout<<"cannot insert more element";
    else{
      top = top+1;
      st[top] = x;
    }
  }

  int top(){
    if(top==-1)
      return 0;
    return st[top];
  }

  void pop(){
    if(top==-1)
      cout<<"stack is empty";
    else
      top = top-1;
  }

  int size(){
    return top+1;
  }

};
