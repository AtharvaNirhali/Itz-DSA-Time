#include<bits/stdio.h>
using namespace std;

// For swapping an array
void f(int i, int arr[], int n){
  if(i>=n/2) return;
  swap(arr[i}, arr[n-i-1]);
  f(i+1, arr, n);
}

int main(){
  int n;
  cin>>n;
  for (int i=0; i<n; i++) cin>>arr[i];
  f(0, arr, n);
  for (int i=0; i<n; i++) cout<< arr[i]<<" ";
  return 0;
} 

// For checking palandrom
bool f(int i, vector<char> &arr, int n){
  if(i>=n/2) return true;
  if(arr[i]!=arr[n-i-1]) return false;
  return f(i+1, arr, n);
}
int main(){
  int n;
  cin>>n;
  for (int i=0; i<n; i++) cin>>arr[i];
  f(0, arr, n);
  for (int i=0; i<n; i++) cout<< arr[i]<<" ";
  return 0;
} 
