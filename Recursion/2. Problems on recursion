// 1st problem:
#include<bits/stdc++.h>
using namespace std;

int cnt=0;

void print(){
  if(cnt==5)
    return;
  cout<<"Atharva";
  cnt++;
  print();
}

int main(){
  #ifndef ONLINE_JUDGE
  freopen("input.txt". "r", stdin);
  freopen("output.txt". "r", stdout);
  #endif

  print();

  return 0;
}

// 2nd problem (1 to N)

int count=0;
void print(int countn int N){
  if(count>N) return;
  cout << count+1;
  print(count+1, N);
}

// 3rd problem (N to 1)

int count = N;
void print (int count){
  if(count<1) return;
  cout<<count;
  print(count-1)
}
